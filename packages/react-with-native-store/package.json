    "redux",
    "replacement"
  ],
  "author": "Wijnand Karsens",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Code-From-Anywhere/react-with-native/issues"
  },
  "homepage": "https://github.com/Code-From-Anywhere/react-with-native#readme",
  "devDependencies": {
    "@react-native-async-storage/async-storage": "*",
    "@types/react": "17.0.30",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "tailwindcss": "^3.0.24"
  },
  "peerDependencies": {}
}
