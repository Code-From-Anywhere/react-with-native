{
  "version": "0.0.0",
  "private": true,
  "name": "react-with-native-github",
  "scripts": {
    "pub": "echo \"please run 'puball packages'\"",
    "build": "turbo run build",
    "buildWithoutCache": "turbo run build --force",
    "dev": "turbo run dev --parallel --concurrency=15",
    "dev:tw": "turbo run dev:tw --parallel --concurrency=15",
    "start": "turbo run start --parallel",
    "start:app": "cd apps/app && yarn start",
    "start:docs": "cd apps/docs && yarn start",
    "start:web": "cd apps/web && yarn start",
    "start:server": "cd apps/server && yarn start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf node_modules yarn.lock && yarn && yarn buildWithoutCache",
    "ship": "yarn build && puball packages && yarn && ship",
    "watch": "nodemon -e tsx,ts -w ./packages/**/src/**/* -x 'yarn build; echo'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Code-From-Anywhere/react-with-native.git"
  },
  "keywords": [
    "react",
    "with",
    "native"
  ],
  "author": "Wijnand Karsens <wijnand@karsens.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Code-From-Anywhere/react-with-native/issues"
  },
  "homepage": "https://github.com/Code-From-Anywhere/react-with-native#readme",
  "devDependencies": {
    "nodemon": "^2.0.16",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "dependencies": {},
  "resolutions": {
    "@types/react": "17.0.30"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "third-party/*/packages/*"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
